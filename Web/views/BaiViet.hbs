<section id="mainContent">
    <div class="content_bottom">
      <div class="col-lg-8 col-md-8">
        <div class="content_bottom_left">
          <div class="single_page_area">
            <ol class="breadcrumb">
              <li><a href="/">Index</a></li>
             
              <li class="active">{{this.baibao.TenBaiBao}} </li>
            </ol>
            <h2 class="post_titile">{{this.baibao.TenBaiBao}}</h2>
            <div class="single_page_content">
              <div class="post_commentbox"> <a href="#"><i class="fa fa-user"></i>Wpfreeware</a> <span><i class="fa fa-calendar"></i>{{this.baibao.NgayDang}}</span> <a href="#"><i class="fa fa-tags"></i>{{this.baibao.T}}</a> </div>
     
