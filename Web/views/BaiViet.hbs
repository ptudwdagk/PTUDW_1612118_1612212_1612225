                      <h4 class="media-heading"><a href="/baibao/{{idBaiBao}}">{{TenBaiBao}}</a></h2></h4>
                    <p>{{NoiDungTomTat}} </p>
                     <div class="Date">{{NgayDang}}</div>
                  </div>
                </div>
              </li>
              {{/each}}
            </ul>
          </div>
        
          
        </div>
