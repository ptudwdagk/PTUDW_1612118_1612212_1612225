{{#section 'js'}}

{{/section}}

<section id="mainContent">
    <div class="content_top">
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm6">
          <div class="latest_slider">
            <div class="slick_slider">
              {{#each bonbaibao}}
              <div class="single_iteam" ><img class="img-top"src="{{AnhDaiDien}}" alt="">
                <h2><a class="slider_tittle" href="/baibao/{{idBaiBao}}">{{TenBaiBao}}</a></h2>
              </div>
              {{/each}}
            </div>
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm6">
          <div class="content_top_right">
            <ul class="featured_nav wow fadeInDown">
              {{#each baimoinhat}}
              <li><img src="{{AnhDaiDien}}" alt="">
                <div class="title_caption"><h3><a href="/baibao/{{idBaiBao}}">{{TenBaiBao}}
                </a></h3></div>

              </li>
             {{/each}}
              
            </ul>
          </div>
        </div>
      </div>
    </div>
    
    <div class="content_bottom">
      <div class="col-lg-8 col-md-8">
        <div class="content_bottom_left">
            
          <div class="games_fashion_area">
             {{#each baithuocchuyenmucnoibat }}
            <div class="games_category">
            
              <div class="single_category">
               
                <h2> <span class="bold_line"><span></span></span> <span class="solid_line"></span> <a class="title_text" href="#">{{TenCM}}</a> </h2>
                <ul class="fashion_catgnav wow fadeInDown">
                  <li>
                    <div class="catgimg2_container"> <a href="/baibao/{{idBaiBao}}"><img alt="" src="{{AnhDaiDien}}"></a> </div>
                    <h2 class="catg_titile"><a href="/baibao/{{idBaiBao}}">{{TenBaiBao}}</a></h2>
                    <div class="comments_box"> <span class="meta_date">{{NgayDangBai}}</span> <span class="meta_comment"><a href="#">No Comments</a></span> <span class="meta_more"><a  href="#">Read More...</a></span> </div>
                    <p>{{NoiDungTomTat}}</p>
                  </li>
                </ul>
                  
              </div>
             
            </div>
               {{/each}}
          </div>
      
        </div>
      </div>
      <div class="col-lg-4 col-md-4">
        <div class="content_bottom_right">
         
          <div class="single_bottom_rightbar">
            <ul role="tablist" class="nav nav-tabs custom-tabs">
              <li class="active" role="presentation"><a data-toggle="tab" role="tab" aria-controls="home" href="#mostPopular">Nổi bật nhất</a></li>
              <li role="presentation"><a data-toggle="tab" role="tab" aria-controls="messages" href="#recentComent">Mới nhất</a></li>
            </ul>

            <div class="tab-content">
              <div id="mostPopular" class="tab-pane fade in active" role="tabpanel">
                <ul class="small_catg popular_catg wow fadeInDown">

                  {{#each muoibaibao}}
                  <li>
                    <div class="media wow fadeInDown"> <a class="media-left" href="/baibao/{{idBaiBao}}"><img src="{{AnhDaiDien}}" alt=""></a>
                      <div class="media-body">
                        <h4 class="media-heading"><a href="/baibao/{{idBaiBao}}">{{TenBaiBao}}</a></h4>
                        <p>{{NoiDungTomTat}} </p> 
                        <div class="Date">{{NgayDangBai}}</div>
                        <div class="Tags">
                          <strong>Tags: </strong>
                          <a href="/baibao/{{idBaiBao}}">Bai Viet 1</a>
                          ,
                          <a href="/baibao/{{idBaiBao}}">Bai Viet 2</a>
                        </div>
                      </div>
                    </div>
                  </li>
                 {{/each}}
                </ul>
              </div>
           <div id="recentComent" class="tab-pane fade" role="tabpanel">
                <ul class="small_catg popular_catg">

                    {{#each baimoinhat}}
                    <li>
                      <div class="media wow fadeInDown"> <a class="media-left" href="/baibao/{{idBaiBao}}"><img src="{{AnhDaiDien}}" alt=""></a>
                        <div class="media-body">
                          <h4 class="media-heading"><a href="/baibao/{{idBaiBao}}">{{TenBaiBao}} </a></h4>
                          <p>{{NoiDungTomTat}} </p>
                          <div class="Date">{{NgayDangBai.Date}}</div>
                         </div>
                      </div>
                    </li>
                    {{/each}}
              
                  </ul>
                </div>
            </div>
          </div>
          
      
        </div>
      </div>
    </div>
  </section>
